{{>paneltheme}}
<div class="pcoded-content">
                        <div class="pcoded-inner-content">
                            <div class="main-body">
                                <div class="page-wrapper">
        <!-- Container-fluid starts -->
        
                    <!-- Authentication card start -->
                    <div class="signup-card card-block auth-body mr-auto ml-auto">
                        <form class="md-float-material" action="/admin/updateLec/:{{lec._id}}" method="POST">
                            <div class="auth-box">
                                <div class="row m-b-20">
                                    <div class="col-md-12">
                                        <h3 class=" text-left text-primary">Edit Lecture</h3>
                                    </div>
                                </div>
                                <hr/>
                                <div class="text-danger alert-danger"><h4><b>{{msg}}</b></h4></div><br>
                                
                                 <div class="input-group">
                                    <input type="text" name="classId" class="form-control" value="{{lec.classid}}">
                                </div>
                                <div class="input-group">
                                    <input type="text" name="fId" class="form-control" value="{{lec.fid}}">
                                </div>
                                <div class="input-group">
                                    <input type="text" name="subId" class="form-control" value="{{lec.subid}}">
                                </div>
                                <div class="row m-t-30">
                                    <div class="col-md-12">
                                        <input type="submit" class="btn btn-primary btn-md  waves-effect text-center m-b-20" value="Update">
                                    </div>
                                </div>
                              
                                
                               
                             
                            </div>
                        </form>
                        <!-- end of form -->
                    </div>
                    <!-- Authentication card end -->
                </div>
                <!-- end of col-sm-12 -->
            </div>
            <!-- end of row -->
        </div>
        <!-- end of container-fluid -->
</div>
