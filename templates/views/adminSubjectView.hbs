{{>paneltheme}}
<div class="pcoded-content">
                        <div class="pcoded-inner-content">
                            <div class="main-body">
                                <div class="page-wrapper">
                                   
                                    <div class="card">
                                        <div class="card-header">
                                            <h1>Subject List &nbsp;&nbsp;&nbsp;<a href="/admin/addSubject"><button class="btn btn-md waves-effect text-center m-b-20 btn-light"><i class="ti-plus"></i></button></a></h1> 
                                            <div class="card-header-right">    <ul class="list-unstyled card-option">        <li><i class="icofont icofont-simple-left "></i></li>        <li><i class="icofont icofont-maximize full-card"></i></li>        <li><i class="icofont icofont-minus minimize-card"></i></li>        <li><i class="icofont icofont-refresh reload-card"></i></li>        <li><i class="icofont icofont-error close-card"></i></li>    </ul></div>
                                        </div>
                                         <div class="card-block table-border-style">
                                            <div class="table-responsive">
                                                <table class="table ">
                                                <thead>
          <tr>
            <th>Subject ID</th>
            <th>Subject Name</th>
            <th>Course ID</th>
            <th>Semester ID</th>
            <th>Actions</th>
          </tr>
          </thead>
        {{# each subject}}
        <tbody>
                <tr>
                    <td>{{this.subid}}</td>
                    <td>{{this.subname}}</td>
                    <td>{{this.cid}}</td>
                    <td>{{this.sid}}</td>
                   
                    <td>
                        &nbsp;
                       <a href="/admin/editSubject/:{{this.subid}}"><i class="ti-pencil-alt text-primary"></i></a> &nbsp; 
                        <a href="/admin/deleteSubject/:{{this.subid}}" onclick="deleteAlert()"><i class="ti-trash text-danger"></i></a>&nbsp;
                    </td>
                </tr>
                </tbody>
        {{/each}}     
</table>
</div>
</div>
</div>
</div>
</div>
</div>
